{
  "name": "@openclaw/agentgate",
  "version": "0.1.0",
  "description": "OpenClaw channel plugin for AgentGate - proxy AI agent messaging through AgentGate",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "openclaw": "workspace:*"
  },
  "peerDependencies": {
    "openclaw": "*"
  },
  "openclaw": {
    "extensions": ["./index.ts"],
    "channels": ["agentgate"],
    "installDependencies": true,
    "channel": {
      "id": "agentgate",
      "label": "AgentGate",
      "selectionLabel": "AgentGate",
      "blurb": "Proxy AI agent messaging through AgentGate",
      "order": 100
    },
    "install": {
      "npmSpec": "@openclaw/agentgate",
      "localPath": "extensions/agentgate-channel",
      "defaultChoice": "local"
    }
  }
}
